{
  "manifest_version": 3,
  "name": "Binance P2P User Logger and Push",
  "version": "1.0",
  "description": "Logs API and pushes data to local server.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://p2p.binance.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}