{
  "manifest_version": 3,
  "name": "Binance Merchant Extension",
  "version": "1.0",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "alarms"
  ],
  "host_permissions": [
    "https://c2c-admin.binance.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
